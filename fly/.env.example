# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# InfluxDB Configuration
INFLUXDB_TOKEN=your-secure-influxdb-token-here
INFLUXDB_ADMIN_TOKEN=your-secure-admin-token-here
INFLUXDB_ORG=linknode
INFLUXDB_BUCKET=energy
INFLUXDB_USER=admin
INFLUXDB_PASSWORD=your-secure-password-here

# Fly.io Configuration
FLY_API_TOKEN=your-fly-api-token-here

# Grafana Configuration
GF_SECURITY_ADMIN_PASSWORD=your-secure-grafana-password-here
GF_AUTH_ANONYMOUS_ENABLED=true
GF_AUTH_ANONYMOUS_ORG_ROLE=Viewer

# Application Configuration
EAGLE_MONITOR_PORT=5000